<Window x:Class="QuantStudio.Shell.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuantStudio.Shell" d:DataContext="{d:DesignInstance Type=local:MainViewModel}"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="450" >
    <StackPanel x:Name="stackPanel">
        <TextBox x:Name="txtTest" Text="{Binding ElementName=sliderTest, Path=Value, Mode=TwoWay}" Margin="10"></TextBox>
        <Slider x:Name="sliderTest" Maximum="100" Minimum="1" Margin="10"></Slider>
    </StackPanel>
</Window>
